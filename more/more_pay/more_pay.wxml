<!--more/more_pay/more_pay.wxml-->
    <view class='songsheet_items_allbox newsong_allbox'>
    <text class='songsheet_container_title'>付费精品</text>
      <view class='songsheet_items_box'>
        <view wx:for="{{pay}}" wx:key="" class='songsheet_item_box' bindtap='handlePlayDj'>
          <image src='{{item.picUrl}}' class="songsheet_container_image"></image>
          <text class='songsheet_container_text newsong_songname'>{{item.name}}</text>
          <text class='newsong_artists'>更新至{{item.lastProgramName}}</text>
          <text class='newsong_artists'>{{item.programCount}}个节目</text>
        </view>
      </view>
    </view>
