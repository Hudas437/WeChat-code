.songer_box{
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-flow: row wrap;
  padding: 10rpx;
}
.songer_index_box{
  text-align:center;
  width: 10%;
}
.songer_index{
line-height: 100px;
color: #9A9A9B;
}
.songer_img_box{
  width: 30%;
  height: 90px;
}
.songer_img{
  width: 100%;
  height: 100%;
  border-radius: 16rpx;
}
.songer_name_box{
  width: 60%;
  height: 100%;
  box-sizing: border-box;
}
.songer_name{
  font-size: 15px;
  padding-left: 20rpx;
}
.songer_score{
  display: block;
  font-size: 12px;
  color: #9A9A9B;
  padding-left:20rpx;
  padding-top: 65rpx;
}